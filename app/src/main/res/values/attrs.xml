<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <declare-styleable name="CircleProgress">
        <attr name="color1" format="reference|color"/>
        <attr name="color2" format="reference|color"/>
        <attr name="color3" format="reference|color"/>
    </declare-styleable>

    <!-- rating bar-->
    <declare-styleable name="RatingBar" tools:ignore="ResourceName">
        <!--尺寸值-->
        <attr name="starImageSize" format="dimension"/>
        <attr name="starCount" format="integer"/>
        <!--资源文件值-->
        <attr name="starEmpty" format="reference"/>
        <!--资源文件值-->
        <attr name="starFill" format="reference"/>
        <!--boolean值-->
        <attr name="clickable" format="boolean"/>
    </declare-styleable>
</resources>